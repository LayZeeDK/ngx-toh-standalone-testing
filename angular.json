{
  "$schema": "./node_modules/nx/schemas/workspace-schema.json",
  "version": 1,
  "projects": {
    "crisis-center-feat": {
      "projectType": "library",
      "root": "libs/crisis-center/feat",
      "sourceRoot": "libs/crisis-center/feat/src",
      "prefix": "toh",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/crisis-center/feat"],
          "options": {
            "jestConfig": "libs/crisis-center/feat/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/crisis-center/feat/**/*.ts",
              "libs/crisis-center/feat/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "crisis-center-routing": {
      "projectType": "library",
      "root": "libs/crisis-center/routing",
      "sourceRoot": "libs/crisis-center/routing/src",
      "prefix": "tour-of-heroes",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/crisis-center/routing"],
          "options": {
            "jestConfig": "libs/crisis-center/routing/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/crisis-center/routing/**/*.ts",
              "libs/crisis-center/routing/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "tour-of-heroes-app": {
      "projectType": "application",
      "root": "apps/tour-of-heroes-app",
      "sourceRoot": "apps/tour-of-heroes-app/src",
      "prefix": "tour-of-heroes",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/tour-of-heroes-app",
            "index": "apps/tour-of-heroes-app/src/index.html",
            "main": "apps/tour-of-heroes-app/src/main.ts",
            "polyfills": "apps/tour-of-heroes-app/src/polyfills.ts",
            "tsConfig": "apps/tour-of-heroes-app/tsconfig.app.json",
            "assets": [
              "apps/tour-of-heroes-app/src/favicon.ico",
              "apps/tour-of-heroes-app/src/assets"
            ],
            "styles": ["apps/tour-of-heroes-app/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/tour-of-heroes-app/src/environments/environment.ts",
                  "with": "apps/tour-of-heroes-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "tour-of-heroes-app:build:production"
            },
            "development": {
              "browserTarget": "tour-of-heroes-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tour-of-heroes-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/tour-of-heroes-app/**/*.ts",
              "apps/tour-of-heroes-app/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/tour-of-heroes-app"],
          "options": {
            "jestConfig": "apps/tour-of-heroes-app/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "tour-of-heroes-app-e2e": {
      "root": "apps/tour-of-heroes-app-e2e",
      "sourceRoot": "apps/tour-of-heroes-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/tour-of-heroes-app-e2e/cypress.config.ts",
            "devServerTarget": "tour-of-heroes-app:serve:development",
            "testingType": "e2e"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tour-of-heroes-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/tour-of-heroes-app-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["tour-of-heroes-app"]
    }
  }
}
